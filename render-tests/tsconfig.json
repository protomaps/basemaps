{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "checkJs": true,
      "isolatedModules": false,
      "noEmit": false,
      "outDir": "dist",
      "esModuleInterop": true,
      "importHelpers": false,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "skipLibCheck": false,
      "strict": false,
      "target": "ES2017",
      "lib": [
        "ESNext",
        "DOM",
        "DOM.Iterable"
      ]
    },
    "ts-node": {
      "experimentalSpecifierResolution": "node",
      "esm": true,
      "compilerOptions": {
        "target": "es2019",
        "types": [
          "node"
        ]
      }
    },
    "include": [
      "run_render_tests.ts"
    ]
  }
  